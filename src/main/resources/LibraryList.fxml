<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="net.atlassian.libraryapp1.Controllers.LibraryListController">
    <children>
        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="55.0" prefWidth="600.0" text="Library list">
            <font>
                <Font name="System Bold" size="20.0"/>
            </font>
        </Label>
        <Button prefWidth="150" layoutX="300.0" layoutY="140.0" mnemonicParsing="false" text="Show books of library"/>
        <TextField prefWidth="150" layoutX="300.0" layoutY="100.0"/>
        <Label prefWidth="150" layoutX="300.0" layoutY="60.0" text="Write a library's name:">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <TableView layoutY="55.0" prefHeight="400.0" prefWidth="259.0">
            <columns>
                <TableColumn prefWidth="110.0" text="LibraryName"/>
                <TableColumn prefWidth="40.0" text="Email"/>
                <TableColumn prefWidth="108.0" text="Phone Number"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>
